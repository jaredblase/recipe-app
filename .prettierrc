{
	"trailingComma": "es5",
	"semi": false,
	"useTabs": true,
	"tabWidth": 2,
	"singleQuote": true,
	"printWidth": 100,
	"plugins": ["prettier-plugin-svelte", "prettier-plugin-tailwindcss"],
	"overrides": [{ "files": "*.svelte", "options": { "parser": "svelte" } }],
	"arrowParens": "avoid",
	"endOfLine": "crlf"
}
